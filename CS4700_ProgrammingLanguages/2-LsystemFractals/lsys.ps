%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%                                Fractal Plant 2                              %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
/fractalPlantDraw2
{
  /c exch def

  c (F) eq
  { 0 len rlineto } if

  c (-) eq
  { a neg rotate } if

  c (+) eq
  { a rotate } if

  c ([) eq
  {
    currentrgbcolor
    currentpoint
    gsave
    stroke
    moveto
    setrgbcolor
  } if

  c (]) eq
  {
    currentrgbcolor
    grestore
    setrgbcolor
  } if

  c (A) eq
  { 0 col 0 col 255 col setrgbcolor} if
  %{ 148 col 60 col 0 col setrgbcolor} if

  c (B) eq
  { 255 col 0 col 0 col setrgbcolor} if
  %{ 0 col 148 col 0 col setrgbcolor} if

  c (C) eq
  %{ 0 col 0 col 0 col setrgbcolor} if
  { 0 col 178 col 0 col setrgbcolor} if

} def

/fractalPlantRules2
{
  /c exch def
  c (X) eq
  {
    (X)(-)(])(X)(F)(+)(C)([)(F)(A)(+)(])(X)(C)(+)(])(X)([)(B)([)(-)(F)(A)
    22 inc
    size 23 roll
} if

c (F) eq
{
  (F)(F)
  1 inc
  size 2 roll
} if

c (A) eq
  { (A) size 1 roll } if

c (B) eq
  { (B) size 1 roll } if

c (C) eq
  { (C) size 1 roll } if

c (-) eq
  { (-) size 1 roll } if

  c (+) eq
{ (+) size 1 roll } if

c ([) eq
  { ([) size 1 roll } if

c (]) eq
  { (]) size 1 roll } if

} def

/fractalPlant2
{
  /len exch def
  /a exch def
  /it exch def
  (X)
  1 inc
  0 1 it 
  {
    pop
    0 1 size 1 sub { pop fractalPlantRules2 } for
} for 
stack
0 1 size 1 sub
{
  pop
  fractalPlantDraw2
} for
stroke
} def

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%                                 Binary Tree                                 %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
/binaryDraw
{
  /c exch def

  c (0) eq
  { len 0 rlineto stroke} if

  c (1) eq
  { len 0 rlineto } if

  c ([) eq
  {
    currentpoint
    gsave
    stroke
    moveto
    a rotate
  } if

  c (]) eq
  {
    grestore
    a neg rotate
  } if
} def

/binaryRules
{
  /c exch def
  c (0) eq
  {
    (0)(])(0)([)(1)
  4 inc
  size 5 roll
} if

c (1) eq
{
  (1)(1)
  1 inc
  size 2 roll
} if

c ([) eq
  { ([) size 1 roll } if

c (]) eq
  { (]) size 1 roll } if

} def

/binary
{
  /len exch def
  /a exch def
  /it exch def
  (0)
  1 inc
  0 1 it 
  {
    pop
    0 1 size 1 sub { pop binaryRules } for
  } for 

  0 1 size 1 sub
  {
    pop
    binaryDraw
  } for
stroke
} def

