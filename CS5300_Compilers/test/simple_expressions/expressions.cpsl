VAR a,b:integer;

BEGIN
  write("2+2=",2+2,'\n');
  write("2*2=",2*2,'\n');
  write("2/2=",2/2,'\n');
  write("2-2=",2-2,'\n');
  write("Enter A:");
  read(a);
  write("\nEnter b:");
  read(b);
  write('\n');
  
  write("a+b=",a+b,'\n');
  write("a-b=",a-b,'\n');
  write("a*b=",a*b,'\n');
  write("a/b=",a/b,'\n');
  write("a%b=",a%b,'\n');
  write("a=b=",a=b,'\n');
  write("a<b=",a<b,'\n');
  write("a>b=",a>b,'\n');
  write("a<>b=",a<>b,'\n');
  write("a<=b=",a<=b,'\n');
  write("a>=b=",a>=b,'\n');
END.
